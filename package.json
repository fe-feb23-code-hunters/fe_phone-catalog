{
  "name": "fe_phone-catalog",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://fe-feb23-code-hunters.github.io/fe_phone-catalog",
  "dependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.34",
    "@types/react": "^18.2.9",
    "@types/react-dom": "^18.2.4",
    "classnames": "^2.3.2",
    "gh-pages": "^5.0.0",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "stylelint": "stylelint './**/*.scss'",
    "stylelint:fix": "stylelint './**/*.scss' --fix",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "lint-staged": {
    "*.(ts|tsx|js|jsx)": [
      "prettier --write",
      "npm run lint"
    ],
    "*.(scss)": [
      "npm run stylelint"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@mate-academy/eslint-config-react": "^0.0.9",
    "@mate-academy/eslint-config-react-typescript": "^1.0.11",
    "@mate-academy/stylelint-config": "^0.0.9",
    "eslint": "^8.42.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.2",
    "node-sass": "^7.0.3",
    "prettier": "^2.8.8",
    "sass": "^1.63.2",
    "stylelint": "^13.13.1"
  }
}
